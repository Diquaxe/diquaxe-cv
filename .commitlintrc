{
  "extends": ["gitmoji"],
  "rules": {
    "type-enum": [
      2, "always", [ "FEATURE", "FIX", "UPDATE", "REFACTOR", "HOTFIX", "COPYRIGHT", "TYPO", "PACKAGE", "ASSETS", "ADAPTIVE", "SEO", "CONFIG", "TESTS", "DOCS", "LINTFIX" ]
    ],
    "type-case": [2, "always", "upper-case"],
    "header-max-length": [0, "always", 75]
  }
}